{
  "manifest_version": 3,
  "name": "Dummy Test Extension",
  "version": "1.0.0",
  "description": "A simple test extension for Chrome Extension Builder",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
} 